{
  "realm": "phase1-realm",
  "enabled": true,
  "registrationAllowed": false,
  "loginWithEmailAllowed": true,
  "resetPasswordAllowed": true,
  "roles": {
    "realm": [
      {
        "name": "ROLE_USER"
      },
      {
        "name": "ROLE_ADMIN"
      }
    ]
  },
  "clients": [
    {
      "clientId": "localys-frontend",
      "name": "Phase1 Frontend SPA",
      "enabled": true,
      "publicClient": true,
      "protocol": "openid-connect",
      "redirectUris": [
        "http://localhost:4200/*",
        "http://localhost:4300/*"
      ],
      "webOrigins": [
        "*"
      ],
      "attributes": {
        "pkce.code.challenge.method": "S256"
      },
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "rootUrl": "http://localhost:4300"
    }
  ],
  "users": [
    {
      "username": "testuser",
      "enabled": true,
      "emailVerified": true,
      "email": "testuser@example.com",
      "credentials": [
        {
          "type": "password",
          "value": "test123",
          "temporary": false
        }
      ],
      "realmRoles": [
        "ROLE_USER"
      ]
    },
    {
      "username": "admin",
      "enabled": true,
      "emailVerified": true,
      "email": "admin@example.com",
      "credentials": [
        {
          "type": "password",
          "value": "admin123",
          "temporary": false
        }
      ],
      "realmRoles": [
        "ROLE_ADMIN",
        "ROLE_USER"
      ]
    }
  ]
}